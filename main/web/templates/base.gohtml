    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Meta Tags Links preview -->
    <meta property="og:title" content="{{template "title"}}">
    <meta property="og:description" content="{{template "metaDescription"}}">
    <meta name="description" content="{{template "metaDescription"}}">
    <!-- Copyright -->
    <meta name="author" content="Noah Elijah Till" />
    <meta name="copyright" content="Weebs Kingdom" />
    <!-- Search -->
    <meta name="robots" content="index,follow" />
    <meta name="keywords" content="weebs, weebskingdom, weebs kingdom, anime, twitch bot, discord bot, discord, community, {{template "metaKeywords"}}">
    <!-- Google -->
    <meta name="googlebot" content="index,follow">

    <!-- Bootstrap -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet"
